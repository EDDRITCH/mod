entity = {
	###Name - entity name, structed: unitname_entity
	name = "power_armour_entity"
	####paradox mesh - inside a .gfx file (powerarmor_meshes.gfx)
	pdxmesh = "powerarmor_t51_mesh"
	
	####Animations and triggers
	
	###state to be in when nothing else is triggered
	default_state = "idle"
	####state to use when attacking an enemy
	state = { name = "attack" 	
		###animation = "" - animation inside gfx file (powerarmor_meshes.gfx)
		animation = "attack" 
		###How much animation blends between, transitions smoothly instead of jumping
		animation_blend_time = 0.0  
		###percentage speed, ex: 90 frames at 30fps is 3seconds*animation speed
		animation_speed = 1.0 
		###Does it loop while state is active
		looping = no 
		###State to go to after animation is complete
		next_state = "attack" 
		##random chance to occur, add all same named states chance, pick one. ex: 2+1 = 3, this has 2/3rds chance to happen
		chance = 2 }
	###second attack state (shooting gun)
	state = { name = "attack"			animation = "attack2" animation_blend_time = 0.0  animation_speed = 1.0 looping = no next_state = "attack" chance = 1 }
	###defensive state
	state = { name = "defend"			animation="shoot" }
	####supporting another attacking unit
	state = { name = "support_attack"	animation="shoot"}
	###movement on map with no enemies
	state = { name = "move"				animation = "move" 	animation_blend_time = 0.0 animation_speed = 1.0}
	###retreat "limp"
	###PLACEHOLDER
	state = { name = "retreat"		animation="move" }
	#### death state (despawns after this)
	state = { name = "death"		}
	###HIGH PRIORITY
	state = { name = "idle" animation="idle1" chance = 6 looping=no}
	state = { name = "idle" animation ="idle2" chance = 3 next_state="idle1"}
	state = { name = "idle" animation ="idle3" chance = 1 next_state="idle1"}
	### training state, normally many of these
	state = { name = "training"	 }
	
	####These attachments are called inside the state = {} to change the bone the weapon is attached to. ex: back, hands
	#attach = { 	name = "rifle1"	Right_Hand_node = "laser_rifle_entity" }	
